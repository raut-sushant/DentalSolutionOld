package com.yash.demorest;

import java.util.ArrayList;
import java.util.List;

public class AlienRepository {

	List<Alien> aliens;
	public AlienRepository() {
		aliens = new ArrayList<Alien>();
	
		Alien a1 = new Alien();
		a1.setName("Sushant");
		a1.setAge(34);
		
		Alien a2 = new Alien();
		a2.setName("Poonam");
		a2.setAge(33);
		
		Alien a3 = new Alien();
		a3.setName("Parth");
		a3.setAge(1);
		
		aliens.add(a1);
		aliens.add(a2);
		aliens.add(a3);
	}

	public List<Alien> getAliens(){
		return aliens;
	}
}
